{
  "name": "totalo-mobile-erp",
  "version": "0.1.1",
  "description": "Totalo Accounting Application",
  "main": "index.js",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "start:prod": "npm run build && npm run open:dist",
    "start:mock": "babel-node --inspect tools/mock.js",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\"",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "antd-mobile": "2.1.2",
    "babel-runtime": "^6.11.6",
    "dateformat": "1.0.12",
    "es6-promise": "3.2.1",
    "history": "4.7.2",
    "isomorphic-fetch": "2.2.1",
    "js-cookie": "2.1.2",
    "jsdom": "9.12.0",
    "lodash": "4.17.4",
    "moment": "2.19.3",
    "object-assign": "4.1.0",
    "prop-types": "15.5.10",
    "rc-form": "1.5.0",
    "rc-notification": "3.0.0",
    "react": "15.6.1",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-stickynode": "1.4.0",
    "redux": "3.7.2",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.2.0",
    "standard-error": "1.1.0",
    "styled-components": "2.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "6.2.5",
    "babel-plugin-antd": "0.5.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-import": "^1.0.1",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "6.13.0",
    "babel-preset-stage-2": "6.13.0",
    "babel-register": "6.14.0",
    "browser-sync": "2.18.13",
    "bundle-loader": "0.5.5",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.12",
    "cross-env": "2.0.1",
    "css-loader": "0.24.0",
    "enzyme": "2.9.1",
    "eslint": "3.4.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.2.1",
    "eslint-plugin-react": "6.2.0",
    "eslint-watch": "2.1.14",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.30.1",
    "http-proxy-middleware": "0.17.1",
    "isparta": "4.0.0",
    "json-server": "0.12.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "less-vars-to-js": "1.2.0",
    "mocha": "3.0.2",
    "nock": "8.0.0",
    "npm-run-all": "3.0.0",
    "ora": "0.3.0",
    "postcss": "^5.2.5",
    "postcss-loader": "^1.1.0",
    "postcss-pxtorem": "^3.3.1",
    "react-dom": "15.6.1",
    "react-test-renderer": "15.6.1",
    "redux-logger": "2.6.1",
    "redux-mock-store": "1.1.4",
    "rimraf": "2.5.4",
    "serve-static": "1.13.1",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "style-loader": "0.18.2",
    "svg-fill-loader": "0.0.8",
    "svg-sprite-loader": "3.4.1",
    "svgo-loader": "2.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "2.12.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "antd-mobile": {
    "upgrade-tip": false
  }
}
